{
  "name": "codeceptjs-image",
  "version": "3.0.2",
  "private": true,
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/puppeteer": "~3.0.2",
    "allure-commandline": "~2.13.0",
    "axe-puppeteer": "~1.1.1",
    "axe-webdriverjs": "~2.3.0",
    "body-parser": "~1.19.0",
    "codeceptjs": "3.0.2",
    "eslint": "~7.10.0",
    "eslint-plugin-codeceptjs": "~1.3.0",
    "eslint-plugin-jest": "~24.1.0",
    "extend": "~3.0.2",
    "gulp": "~4.0.2",
    "gulp-angular-protractor": "~2.0.0",
    "gulp-debug": "~4.0.0",
    "gulp-eslint": "~6.0.0",
    "gulp-util": "~3.0.8",
    "jasmine-allure-reporter": "~1.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-reporters": "~2.3.2",
    "mocha-junit-reporter": "~2.0.0",
    "mocha-multi": "~1.1.3",
    "needle": "~2.5.2",
    "plugin-error": "~1.0.1",
    "protractor": "5.4.4",
    "protractor-beautiful-reporter": "~1.3.6",
    "protractor-http-mock": "~0.10.0",
    "puppeteer": "4.0.1",
    "sockjs": "~0.3.21",
    "stomp-broker-js": "~1.3.0",
    "webdriver-manager": "~12.1.7",
    "yargs": "~16.0.3"
  },
  "scripts": {
    "lint": "gulp lint",
    "codeceptjs:command": "codeceptjs --steps --plugins allure",
    "codeceptjs:one": "yarn run codeceptjs:command run --debug --verbose",
    "codeceptjs:all": "yarn run codeceptjs:command run-multiple --all",
    "codeceptjs": "yarn run codeceptjs:one --grep",
    "codeceptjs:show": "yarn run codeceptjs:one -o '{ \"helpers\": {\"Puppeteer\": {\"show\": true}}}' --grep",
    "codeceptjs:all:show": "yarn run codeceptjs:all -o '{ \"helpers\": {\"Puppeteer\": {\"show\": true}}}'",
    "wd:start": "webdriver-manager update && webdriver-manager start --standalone",
    "docker-run": "docker run --net=host -v $PWD:/tests -v $PWD./../../../target:/target codeceptjslatest/codecept-js:latest codeceptjs run-workers --suites 8  --plugins allure"
  }
}
